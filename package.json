    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --external:@google-cloud/firestore --external:@google-cloud/text-to-speech --external:@google-cloud/logging-bunyan --external:discord.js --external:@discordjs/opus --external:@discordjs/voice --outfile=dist/helmholtz.js",
    "format": "prettier --write \"*.js\" \"src/**/*\" \"tools/**/*\"",
    "lint": "prettier --check \"src/**/*\" \"tools/**/*\" && eslint .",
